<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <listener>
    <listener-class>Session.MySessionListener</listener-class>
  </listener>

  <servlet>
    <servlet-name>Servlet</servlet-name>
    <servlet-class>Servlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Hardware</servlet-name>
    <servlet-class>Hardware</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Sign</servlet-name>
    <servlet-class>Sign</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>Login</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Chance</servlet-name>
    <servlet-class>Chance</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>News</servlet-name>
    <servlet-class>News</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>NewsData</servlet-name>
    <servlet-class>NewsData</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Talk</servlet-name>
    <servlet-class>Talk</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Post</servlet-name>
    <servlet-class>Post</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Servlet</servlet-name>
    <url-pattern>/a</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Hardware</servlet-name>
    <url-pattern>/hardware</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Sign</servlet-name>
    <url-pattern>/sign</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Chance</servlet-name>
    <url-pattern>/chance</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>News</servlet-name>
    <url-pattern>/news</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>NewsData</servlet-name>
    <url-pattern>/newsdata</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Talk</servlet-name>
    <url-pattern>/talk</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Post</servlet-name>
    <url-pattern>/post</url-pattern>
  </servlet-mapping>
</web-app>